# Application
ENV=development
DEBUG=true
HOST=0.0.0.0
PORT=8000

# RabbitMQ
RABBITMQ_URL=amqp://guest:guest@localhost:5672

# Database (for updating job status)
DATABASE_URL=postgresql://postgres:postgres@localhost:5432/fashion_ai_dev

# Storage (MinIO)
MINIO_ENDPOINT=localhost:9000
MINIO_ACCESS_KEY=minioadmin
MINIO_SECRET_KEY=minioadmin
MINIO_BUCKET=fashion-ai-dev
MINIO_SECURE=false

# Redis
REDIS_URL=redis://:redis123@localhost:6379

# AI Models
AI_MODEL_PATH=./models
VITON_MODEL=viton-hd

# LLM Provider (gemini or openai)
LLM_PROVIDER=gemini
GEMINI_API_KEY=your-gemini-api-key
# OPENAI_API_KEY=your-openai-api-key

# Rate Limiting
MAX_TRYON_PER_MINUTE=5
MAX_SIZE_REC_PER_MINUTE=20
MAX_CHAT_PER_MINUTE=30
